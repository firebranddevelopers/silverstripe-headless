{
  "name": "silverstripe/silverstripe-headless",
  "description": "Silverstripe CMS headless features",
  "type": "silverstripe-vendormodule",
  "license": "MIT",
  "keywords": [
    "silverstripe",
    "headless"
  ],
  "authors": [
    {
      "name": "Uncle Cheese"
    }
  ],
  "require": {
    "silverstripe/cms": "^5",
    "silverstripe/graphql": "^5",
    "silverstripe/cms-events": "dev-master",
    "unclecheese/display-logic": "~3.0",
    "sminnee/silverstripe-apikey": "dev-master",
    "symbiote/silverstripe-grouped-cms-menu": "dev-master",
    "firebase/php-jwt": "^6.8",
    "guzzlehttp/guzzle": "^7.7"
  },
  "autoload": {
    "psr-4": {
      "SilverStripe\\Headless\\": "src/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "silverstripe/vendor-plugin": true
    }
  }
}
